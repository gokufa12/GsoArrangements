{
  "name": "GsoArrangements",
  "version": "1.0.0",
  "description": "Tracker for Videogame Music Arrangements",
  "keywords": [
    "OpenShift",
    "Node.js",
    "application",
    "openshift"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/openshift/origin-server"
  },
  "engines": {
    "node": ">= 0.6.0",
    "npm": ">= 1.0.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "supervisor": "supervisor ./bin/www",
    "db_init:user": "createuser -U postgres -P -D -A db_user && psql -U postgres -f ./db_scripts/alter_user.sql",
    "db_init:replace": "node ./user_pass_replace.js db_user admin",
    "db_init:db": "psql -U db_user -f ./db_scripts/create_db_script.sql",
    "db_init:instruments": "psql -U db_user -f ./db_scripts/instruments_init_script.sql",
    "db_init": "npm run db_init:replace && npm run db_init:user && npm run db_init:db && npm_run db_init:instruments"
  },
  "dependencies": {
    "async-lock": "^0.3.8",
    "bluebird": "^3.3.4",
    "body-parser": "~1.13.2",
    "bower": "^1.8.0",
    "browser-sync": "^2.18.7",
    "cookie-parser": "~1.3.5",
    "csv-parse": "^1.1.0",
    "debug": "~2.2.0",
    "ejs-locals": "^1.0.2",
    "express": "~4.13.1",
    "express-jwt": "^3.3.0",
    "gulp-inject": "^4.2.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^5.7.0",
    "morgan": "~1.6.1",
    "multiparty": "^4.1.2",
    "ng-file-upload": "^12.0.4",
    "nodemailer": "^2.5.0",
    "path": "^0.12.7",
    "pg-promise": "^3.0.4",
    "replace": "^0.3.0",
    "serve-favicon": "~2.3.0",
    "tr-ng-grid": "^3.1.6",
    "underscore": "^1.8.3",
    "wiredep": "^4.0.0"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-jshint": "^2.0.4",
    "gulp-livereload": "^3.8.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.2.1"
  },
  "bundleDependencies": [],
  "private": true,
  "main": "server.js"
}
